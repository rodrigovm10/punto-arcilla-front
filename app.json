{
  "expo": {
    "name": "punto-arcilla-front",
    "slug": "punto-arcilla-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "punto-arcilla",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Sora-Bold.ttf",
            "./assets/fonts/Sora-ExtraBold.ttf",
            "./assets/fonts/Sora-ExtraLight.ttf",
            "./assets/fonts/Sora-Light.ttf",
            "./assets/fonts/Sora-Medium.ttf",
            "./assets/fonts/Sora-Regular.ttf",
            "./assets/fonts/Sora-SemiBold.ttf",
            "./assets/fonts/Sora-Thin.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
